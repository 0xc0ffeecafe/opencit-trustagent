<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <artifactId>MSCmdUtility</artifactId>
  <packaging>jar</packaging>

  <name>Management Service Command Utility</name>
  
    <parent>
        <groupId>com.intel.mtwilson.services</groupId>
        <artifactId>mtwilson-services</artifactId>
        <version>0.5.4-SNAPSHOT</version>
    </parent>
    
  <url>http://maven.apache.org</url>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <inherited>true</inherited>
                <version>2.3.2</version>     
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                    <skip>false</skip>
                </configuration>
            </plugin>
        </plugins>
    </build>
    
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <dependencies>
      <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>vmware-util-jar</artifactId>
            <version>${project.version}</version>         
      </dependency>
    <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.8.2</version>
            <scope>test</scope>
    </dependency>
    <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <version>1.6.4</version>
    </dependency>
    <dependency>
            <groupId>com.vmware</groupId>
            <artifactId>VSphere5.1beta</artifactId>
            <version>2.5</version>
    </dependency>
    <dependency>
            <groupId>com.intel.mtwilson.integration</groupId>
            <artifactId>api-datatypes</artifactId>
            <version>${project.version}</version>
    </dependency>
    <dependency>
            <groupId>com.intel.mtwilson.integration</groupId>
            <artifactId>api-client</artifactId>
            <version>${project.version}</version>
    </dependency>
    <dependency>
            <groupId>com.intel.mtwilson.services</groupId>
            <artifactId>webservice-util</artifactId>
            <version>${project.version}</version>
    </dependency>   
    </dependencies>
</project>
